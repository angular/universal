{
  "name": "universal-integration",
  "version": "0.0.0",
  "description": "Integration tests for @nguniversal",
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/universal.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "e2e": "ng e2e",
    "test": "npm run build && npm run serve:test",
    "serve:test": "concurrently \"npm run serve:ssr\" \"npm run e2e\" --kill-others --success first",
    "serve:ssr": "node dist/common/server/main.js",
    "serve:ssr:dev": "ng run common:serve-ssr:production",
    "build:ssr": "npm run build:client-and-server-bundles",
    "build": "ng build --configuration production && ng run common:server:production"
  },
  "private": true,
  "resolutions": {
    "@nguniversal/common": "workspace:*"
  },
  "dependencies": {
    "@angular/animations": "*",
    "@angular/common": "*",
    "@angular/compiler": "*",
    "@angular/compiler-cli": "*",
    "@angular/core": "*",
    "@angular/platform-browser": "*",
    "@angular/platform-browser-dynamic": "*",
    "@angular/platform-server": "*",
    "@angular/router": "*",
    "@nguniversal/common": "workspace:*",
    "express": "*",
    "rxjs": "*",
    "zone.js": "*"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "*",
    "@angular/cli": "*",
    "@angular/compiler-cli": "*",
    "@types/express": "*",
    "@types/node": "*",
    "@types/jasmine": "*",
    "@nguniversal/builders": "workspace:*",
    "codelyzer": "6.0.2",
    "concurrently": "7.3.0",
    "jasmine-core": "4.3.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "protractor": "*",
    "puppeteer": "*",
    "ts-node": "10.9.1",
    "typescript": "*"
  }
}
