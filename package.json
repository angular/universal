{
  "name": "angular2-universal",
  "main": "index.js",
  "version": "0.0.1",
  "description": "Universal (isomorphic) javascript support for Angular2",
  "homepage": "https://github.com/angular/universal",
  "license": "Apache-2.0",
  "contributors": [
    "Tobias Bosch <tbosch@google.com>",
    "PatrickJS <github@gdi2290.com>",
    "Jeff Whelpley <jeff@gethuman.com>"
  ],
  "scripts": {
    "prestart": "gulp build",
    "build": "gulp build.typescript && gulp build.preboot",
    "serve": "gulp serve",
    "start": "gulp serve",
    "watch": "gulp watch",
    "debug": "gulp debug",
    "clean": "gulp clean",
    "preboot": "gulp build.preboot",
    "test": "gulp karma",
    "ci": "gulp lint # && gulp karma && gulp protractor",
    "changelog": "gulp changelog",
    "e2e": "gulp protractor",
    "webdriver-update": "gulp protractor.update",
    "webdriver-start": "gulp protractor.start",
    "preprotractor": "gulp protractor.update",
    "protractor": "gulp protractor",
    "remove-dist": "rm -rf ./dist",
    "remove-tsd-typing": "rm -rf ./tsd_typings",
    "remove-angular2": "rm -rf ./node_modules/angular2",
    "remove-angular-typings": "rm -rf ./angular/modules/angular2/typings",
    "remove-angular-dist": "rm -rf ./angular/dist",
    "remove-web-modules": "rm -rf ./web_modules",
    "update-angular": "git submodule foreach 'npm install && gulp build.js || true'",
    "pull-angular": "git submodule foreach 'git pull https://github.com/angular/angular.git master'",
    "angular-submodule": "git submodule init && git submodule update && npm run pull-angular && npm run remove-angular-typings && npm run remove-angular2 && npm run update-angular && git submodule update",
    "bower": "bower install",
    "web_modules": "bash ./scripts/update-ng-bundle.sh",
    "tsd": "tsd reinstall",
    "preinstall": "npm run tsd && npm run angular-submodule",
    "postinstall": "tsc || true && npm run remove-web-modules && npm run web_modules && npm run bower && npm run webdriver-update"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/universal"
  },
  "bugs": {
    "url": "https://github.com/angular/universal/issues"
  },
  "devDependencies": {
    "bower": "^1.4.1",
    "brfs": "^1.4.0",
    "browser-sync": "^2.8.2",
    "connect-history-api-fallback": "^1.1.0",
    "connect-livereload": "^0.5.3",
    "conventional-changelog": "^0.2.1",
    "del": "^1.2.0",
    "express": "^4.13.0",
    "gulp-eslint": "^0.15.0",
    "gulp-jasmine": "^2.0.1",
    "gulp-livereload": "^3.8.0",
    "gulp-load-plugins": "^0.10.0",
    "gulp-node-inspector": "0.0.4",
    "gulp-nodemon": "^2.0.3",
    "gulp-notify": "^2.2.0",
    "gulp-protractor": "^1.0.0",
    "gulp-size": "^1.2.3",
    "gulp-tslint": "^3.1.0-beta",
    "gulp-typescript": "^2.8.0",
    "jasmine": "^2.3.1",
    "jasmine-reporters": "^2.0.7",
    "jasmine-spec-reporter": "^2.4.0",
    "karma": "^0.13.3",
    "karma-browserify": "^4.2.1",
    "karma-chrome-launcher": "^0.2.0",
    "karma-jasmine": "^0.3.6",
    "morgan": "^1.6.1",
    "nodemon": "^1.3.7",
    "open": "0.0.5",
    "opn": "^3.0.2",
    "protractor": "^2.1.0",
    "rx": "^2.5.3",
    "selenium-webdriver": "^2.46.1",
    "serve-index": "^1.7.0",
    "serve-static": "^1.10.0",
    "tsd": "^0.6.3",
    "typescript": "^1.5.3",
    "yargs": "^3.14.0",
    "zone.js": "^0.5.2"
  },
  "dependencies": {
    "angular2": "file:angular/dist/js/cjs/angular2",
    "browserify": "^11.0.0",
    "css": "^2.2.1",
    "event-stream": "^3.3.1",
    "gulp": "^3.9.0",
    "gulp-insert": "^0.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.2.0",
    "lodash": "^3.10.0",
    "parse5": "^1.5.0",
    "q": "^1.4.1",
    "reflect-metadata": "^0.1.0",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "xhr2": "^0.1.2"
  }
}
