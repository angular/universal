    "browserify": "^11.0.0",
    "css": "^2.2.1",
    "es6-shim": "^0.33.6",
    "event-stream": "^3.3.1",
    "gulp": "^3.9.0",
    "gulp-insert": "^0.5.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.2.0",
    "lodash": "^3.10.1",
    "parse5": "^1.5.0",
    "q": "^1.4.1",
    "reflect-metadata": "^0.1.2",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.2",
    "vinyl-buffer": "^1.0.0",
