{
  "name": "angular2-universal",
  "main": "index.js",
  "version": "0.0.1",
  "description": "Universal (isomorphic) javascript support for Angular2",
  "homepage": "https://github.com/angular/universal",
  "license": "Apache-2.0",
  "contributors": [
    "Tobias Bosch <tbosch@google.com>",
    "PatrickJS <github@gdi2290.com>",
    "Jeff Whelpley <jeff@gethuman.com>"
  ],
  "scripts": {
    "prestart": "gulp build",
    "build": "gulp build.typescript && gulp build.preboot",
    "serve": "gulp serve",
    "start": "gulp server",
    "watch": "gulp watch",
    "debug": "gulp debug",
    "clean": "gulp clean",
    "preboot": "gulp build.preboot",
    "test": "gulp karma",
    "ci": "gulp ci",
    "changelog": "gulp changelog",
    "e2e": "gulp protractor",
    "webdriver-update": "node node_modules/protractor/bin/webdriver-manager update",
    "webdriver-start": "gulp protractor.start",
    "preprotractor": "gulp protractor.update",
    "protractor": "gulp protractor",
    "remove-dist": "node node_modules/rimraf/bin ./dist",
    "remove-tsd-typing": "node node_modules/rimraf/bin ./tsd_typings",
    "remove-angular2": "node node_modules/rimraf/bin ./node_modules/angular2",
    "remove-angular-typings": "node node_modules/rimraf/bin ./angular/modules/angular2/typings",
    "remove-angular-dist": "node node_modules/rimraf/bin ./angular/dist",
    "remove-web-modules": "node node_modules/rimraf/bin ./web_modules",
    "bower": "node node_modules/bower/bin/bower install",
    "web_modules": "gulp update-ng-bundle",
    "tsc": "node node_modules/typescript/bin/tsc",
    "tsd": "node node_modules/tsd/build/cli reinstall && node node_modules/tsd/build/cli link",
    "postinstall": "npm run tsd && npm run tsc || true && npm run remove-web-modules && npm run web_modules && npm run bower && npm run webdriver-update"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/universal"
  },
  "bugs": {
    "url": "https://github.com/angular/universal/issues"
  },
  "devDependencies": {
    "bower": "^1.4.1",
    "brfs": "^1.4.0",
    "browser-sync": "^2.8.2",
    "connect-history-api-fallback": "^1.1.0",
    "connect-livereload": "^0.5.3",
    "conventional-changelog": "^0.5.0",
    "del": "^2.0.2",
    "express": "^4.13.0",
    "gulp-eslint": "^1.0.0",
    "gulp-jasmine": "^2.0.1",
    "gulp-livereload": "^3.8.0",
    "gulp-load-plugins": "^0.10.0",
    "gulp-node-inspector": "^0.1.0",
    "gulp-nodemon": "^2.0.3",
    "gulp-notify": "^2.2.0",
    "gulp-protractor": "^1.0.0",
    "gulp-size": "^2.0.0",
    "gulp-tslint": "^3.1.0-beta",
    "gulp-typescript": "^2.9.0",
    "jasmine": "^2.3.1",
    "jasmine-reporters": "^2.0.7",
    "jasmine-spec-reporter": "^2.4.0",
    "karma": "^0.13.3",
    "karma-browserify": "^4.3.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "mkdirp": "^0.5.1",
    "morgan": "^1.6.1",
    "nodemon": "^1.3.7",
    "open": "0.0.5",
    "opn": "^3.0.2",
    "phantomjs": "^1.9.17",
    "protractor": "^2.1.0",
    "rimraf": "^2.4.3",
    "selenium-webdriver": "^2.46.1",
    "serve-index": "^1.7.0",
    "serve-static": "^1.10.0",
    "tsd": "^0.6.4",
    "typescript": "^1.6.2",
    "yargs": "^3.14.0"
  },
  "dependencies": {
    "zone.js": "^0.5.8",
