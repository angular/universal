{
  "extends": "./tsconfig.lib",
  "compilerOptions": {
    "importHelpers": false,
    "module": "commonjs",
    "target": "es5",
    "types": ["jasmine", "node"]
  },
  "angularCompilerOptions": {
    "strictMetadataEmit": true,
    "skipTemplateCodegen": true,
    "emitDecoratorMetadata": true,
    "fullTemplateTypeCheck": true
  },
  "include": [
    "**/*.spec.ts",
    "index.ts"
  ]
}