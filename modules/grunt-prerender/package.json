{
  "name": "@angular/grunt-prerender",
  "main": "tasks/index.js",
  "typings": "tasks/index.d.js",
  "version": "0.10.1",
  "description": "Prerender your Universal (isomorphic) Angular 2 app with Grunt",
  "homepage": "https://github.com/angular/universal",
  "license": "MIT",
  "contributors": [
    "Tobias Bosch <tbosch@google.com>",
    "PatrickJS <github@gdi2290.com>",
    "Jeff Whelpley <jeff@gethuman.com>"
  ],
  "scripts": {
    "remove-modules": "rimraf node_modules/@angular/universal-preview node_modules/@angular/universal node_modules/@angular/express-engine node_modules/preboot node_modules/@angular/hapi-engine node_modules/@angular/universal-polyfills node_modules/zone.js",
    "reload": "npm run remove-modules && npm install",
    "prebuild": "rimraf tasks",
    "build": "tsc && echo tsc-passed || echo tsc-errors",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/universal"
  },
  "bugs": {
    "url": "https://github.com/angular/universal/issues"
  },
  "keywords": [
    "gruntplugin",
    "angular2 grunt",
    "angular2 universal",
    "angular 2 universal",
    "angular 2",
    "angular 2 prerender",
    "angular 2 grunt prerender"
  ],
  "devDependencies": {
    "preboot": "file:../preboot",
    "@angular/universal": "file:../universal",
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/platform-server": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "rimraf": "^2.5.2",
    "angular2": "2.0.0-beta.13",
    "rxjs": "~5.0.0-beta.8",
    "zone.js": "~0.6.10",
    "typescript": "^1.8.9"
  },
  "dependencies": {
  },
  "peerDependencies": {
    "@angular/universal": ">=0.90.0"
  }
}
