{
  "name": "@angular/prerender-webpack-plugin",
  "main": "dist/index.js",
  "version": "0.10.0",
  "description": "Prerender your Universal (isomorphic) Angular 2 app with Webpack",
  "homepage": "https://github.com/angular/universal",
  "license": "MIT",
  "contributors": [
    "Tobias Bosch <tbosch@google.com>",
    "PatrickJS <github@gdi2290.com>",
    "Jeff Whelpley <jeff@gethuman.com>"
  ],
  "scripts": {
    "remove-modules": "rimraf node_modules/@angular/universal-preview node_modules/@angular/universal node_modules/@angular/express-engine node_modules/preboot node_modules/@angular/hapi-engine node_modules/@angular/universal-polyfills",
    "reload": "npm run remove-modules && npm install",
    "prebuild": "rimraf dist",
    "build": "tsc && echo tsc-passed || echo tsc-errors",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/universal"
  },
  "bugs": {
    "url": "https://github.com/angular/universal/issues"
  },
  "devDependencies": {
    "@angular/common": "2.0.0-rc.3",
    "@angular/compiler": "2.0.0-rc.3",
    "@angular/core": "2.0.0-rc.3",
    "@angular/http": "2.0.0-rc.3",
    "@angular/platform-browser": "2.0.0-rc.3",
    "@angular/platform-browser-dynamic": "2.0.0-rc.3",
    "@angular/platform-server": "2.0.0-rc.3",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "preboot": ">=2.1.2",
    "@angular/universal": "file:../universal",
    "rimraf": "^2.5.1",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "~0.6.12",
    "typescript": "^1.8.9"
  },
  "dependencies": {
  },
  "peerDependencies": {
    "@angular/universal": "^0.104.2"
  }
}
