{
  "name": "@angular/gulp-prerender",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "version": "0.10.1",
  "description": "Prerender your Universal (isomorphic) Angular 2 app",
  "homepage": "https://github.com/angular/universal",
  "license": "MIT",
  "contributors": [
    "Tobias Bosch <tbosch@google.com>",
    "PatrickJS <github@gdi2290.com>",
    "Jeff Whelpley <jeff@gethuman.com>"
  ],
  "scripts": {
    "remove-modules": "rimraf dist node_modules/preboot node_modules/@angular/universal-preview node_modules/@angular/universal",
    "reload": "npm run remove-modules && npm install",
    "prebuild": "rimraf dist",
    "build": "tsc && echo tsc-passed || echo tsc-errors",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/universal"
  },
  "bugs": {
    "url": "https://github.com/angular/universal/issues"
  },
  "devDependencies": {
    "preboot": "file:../preboot",
    "@angular/universal": "file:../universal",
    "rimraf": "^2.5.2",
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/platform-server": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "rxjs": "~5.0.0-beta.8",
    "zone.js": "~0.6.12",
    "typescript": "^1.8.10"
  },
  "dependencies": {
    "gutil": "^1.6.4",
    "through2": "^2.0.0"
  },
  "peerDependencies": {
    "@angular/universal": ">=0.90.0"
  }
}
